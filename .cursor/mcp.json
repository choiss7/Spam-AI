{
  "mcpServers": {
    "weather-server": {
      "command": "node",
      "args": [
        "~/mcp-quickstart/weather-server-typescript/build/index.js"
      ]
    },
    "Sequential Thinking": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/server-sequential-thinking",
        "--config",
        "\"{}\""
      ]
    },
    "GitHub": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/github",
        "--config",
        "{\"githubPersonalAccessToken\":\"YOUR_TOKEN_HERE\"}"
      ]
    }
  }
} 